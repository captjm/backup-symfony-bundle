{
  "name": "captjm/backup-symfony-bundle",
  "type": "symfony-bundle",
  "description": "Backup symfony databases and upload folders",
  "keywords": [
    "backend",
    "admin",
    "backup"
  ],
  "homepage": "https://github.com/captjm/backup-symfony-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "David Makaridze",
      "homepage": "https://github.com/captjm"
    }
  ],
  "require": {
    "php": ">=8.1",
    "ext-zip": "*",
    "easycorp/easyadmin-bundle": "^4.7",
    "symfony/finder": "^6.3",
    "symfony/flex": "^2.3",
    "symfony/framework-bundle": "^6.3",
    "symfony/http-kernel": "^6.3",
    "symfony/routing": "^6.3",
    "symfony/twig-bundle": "^6.3",
    "symfony/yaml": "^6.3",
    "twig/twig": "^3.7"
  },
  "autoload": {
    "psr-4": {
      "Captjm\\BackupSymfonyBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Captjm\\BackupSymfonyBundle\\Tests\\": "tests/"
    }
  },
  "conflict": {
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "symfony/flex": true
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    },
    "symfony": {
      "docker": true
    }
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    }
  }
}
